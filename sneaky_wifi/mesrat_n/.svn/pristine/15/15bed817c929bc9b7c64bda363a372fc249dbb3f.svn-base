/* Mon CSS */

#logo_wifi{
	margin: -45px 0 0 0;
	width: 15%;
	height: auto;
}

#zone_map {
	width: 800px;
	height: 600px;
}

#recherche_margin {	
	min-height: 100%;
}

@media screen and (max-width: 768px){
	#zone_map{
		max-width: 100%;
	}
	#diag {
		max-width: 100%;
	}
	svg {
		max-width: 100%;
	}
}
#wifi {
	width: 60px;
	height: 60px;
	margin-left:3px;
}

#David {

	width: 100px;
}
.respon_info {
	float: right;
	margin-left: 50px;	 
	width: 25%;
	margin-top: -1%;	
}

@media screen and (max-width:1200px){
	 .respon_info{
	 	width: 100%;
	 }
}

#respon_row {
	margin-right: 5px;
}

#machin{
	background: rgba(218,63,63, .8);
	border: rgba(218,63,63, .8);
}
.list-group-item-text:before {
	content:"\f041";
	font-family: FontAwesome;
	font-style: normal;
    font-weight: normal;
    color: green;
    font-size: 18px;
    margin-right: 5px;
}

err {
	font-size: 18px;
	color: rgba(213,63,63, 1);
}

err:before {
	font-family: FontAwesome;
